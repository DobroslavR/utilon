  "license": "MIT",
  "homepage": "https://github.com/DobroslavR/utilon#readme",
  "bugs": {
    "url": "https://github.com/DobroslavR/utilon/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DobroslavR/utilon.git"
  },
